from tkinter import *
from PIL import ImageTk, Image

root = Tk()
root.title('learn tkinter')
root.iconbitmap('ic.ico')

frame = LabelFrame(root, padx=40, pady=40)
frame.pack(padx=30, pady=30)

myButton1 = Button(frame, text="Don't press here!")
myButton2 = Button(frame, text="Don't click here!")

myButton1.grid(row=0, column=0, pady=10)
myButton2.grid(row=1, column=1, pady=10)


root.mainloop()

