from tkinter import *
from PIL import ImageTk, Image

root = Tk()
root.title('learn tkinter')
root.iconbitmap('ic.ico')

button_quit = Button(root, text='C u later', command=root.quit)
button_quit.pack()

myImage = ImageTk.PhotoImage(Image.open('vijay.png'))
myLabel = Label(image=myImage)
myLabel.pack()

root.mainloop()

